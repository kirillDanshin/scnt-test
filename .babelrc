{
    "presets": [
		"react",
		["es2015", { "modules": false }],
		"stage-0",
		"stage-1",
		"stage-2",
		"stage-3",
		"es2017"
	],
    "plugins": [
		"react-hot-loader/babel",
		"transform-function-bind",
		"transform-class-properties",
		"transform-decorators-legacy",
		"transform-flow-strip-types",
		"transform-decorators",
		["transform-runtime", {
			"polyfill": true,
			"regenerator": true
    	}]
	]
}
