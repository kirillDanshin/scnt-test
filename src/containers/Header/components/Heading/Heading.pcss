.heading {
	padding: 25px;
	width: 100%;
	border-bottom: 1px solid #c0c0c0;

	& .wrapper {
		@mixin maxWidth;
		display: flex;
		justify-content: space-between;
		align-items: center;
		width: 100%;
		height: 100%;
		margin: auto;
		font-size: 14px;
	}
}

.logo {
	height: 40px;
}

.bird {
	height: 49px;
}
